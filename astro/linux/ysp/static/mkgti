********************************************************************************
mkgti
********************************************************************************


=====mkgti.shによるGTIの作成=====

script for "mkgti.sh"

xis=(0 1 3)
st=(90 10 35)
sp=(10 35 90)
stp=(-0.1 0.1 0.35)
spp=(0.1 0.35 0.9)
num=3

for i in ${xis};do
for j in `seq 1 ${num}` ;do
mkgti.sh ${stp[${j}]} ${spp[${j}]} x${i}_value.txt x${i}_phase${st[${j}]}_${sp[${j}]}.gti
done
done


#EOF#
