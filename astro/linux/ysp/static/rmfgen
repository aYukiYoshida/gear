********************************************************************************
rmfgen
********************************************************************************


===== rmfの作成 =====

xis=(0 1 2 3)

for i in ${xis} ;do
xisrmfgen spec/x${i}.pi rsp/x${i}.rmf
done


===== rmfの作成 =====

for i in *.pi ;do
xisrmfgen ${i} ${i%.pi}.rmf
done


#EOF#
