********************************************************************************
hdratio
********************************************************************************


=====hdratio.shによる比の計算=====

script for "hdratio.sh"

xis=(0 1 3)
norm=0.8046875

for i in ${xis} ;do
for j in 3050 5070 70120 ;do
hdratio.sh -p x${i}_nbg_${j}_eflc.qdp x${i}_nbg_0530_eflc.qdp ${norm} x${i}_nbg_${j}_eflc_ratio.qdp
done
done



#EOF#
